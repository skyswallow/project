    <div class="jzhd-box">
      <img alt="" src="/images/fmxt1.gif" width="90" height="120"/>
    </div>
    <ul>
      <% (home_educations || []).each do |home_education| %>
        <li id="h_<%= home_education.id %>">
          <a href="/news_lines/<%= home_education.id %>" title="<%=  home_education.title %>" target = "_blank"><%= h(truncate(home_education.title, :length => 12)) %></a>
        </li>

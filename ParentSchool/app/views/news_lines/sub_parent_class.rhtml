<div class="colunm_list_box more_list" style="margin-left: 30px;">
  <h2><%=@type.name%></h2>
  <table width="900">
    <colgroup width="820" />
    <colgroup width="80" />
    <% for line in @lines %>
      <tr><td><a href="/news_lines/<%= line.id %>" target="_blank"><%=h line.title %></a></td><td><%= line.created_at.strftime '%Y.%m.%d' %></td></tr>
    <% end %>
  </table>
  <%= will_paginate @lines %>
</div>